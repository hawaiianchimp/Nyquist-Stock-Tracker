        maxNumberOfLabels=10
        yAxisFromZero=false
        dynamicXAxis=false
        hasXAxisTitle=true
        hasYAxisTitle=true
        xValueDisplayName=1
        yValueDisplayName=1
        xTitleHorizontalOffset=1
        yTitleVerticalOffset=1
      }}